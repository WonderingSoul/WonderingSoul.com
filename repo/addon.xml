<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.wsoul.com" name="wsoul.com Repo" version="1.0.1" provider-name="wsoul.com">
	<extension point="xbmc.addon.repository" name="WSoul.com Repository">
		<info compressed="false">https://github.com/WonderingSoul/WonderingSoul.com/edit/master/repo/addon.xml</info>
		<checksum>https://github.com/WonderingSoul/WonderingSoul.com/blob/master/repo/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/hmemar/husham.com/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>The Husham.com Repo</summary>
		<description>Download and install addons from husham.com repo</description>
		<platform>all</platform>
	</extension>
</addon>

<addon
  id="plugin.video.Husham"
  version="1.0.0"
  name="Husham"
  provider-name="Husham.com">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  	<import addon="script.module.buggalo" version="1.1.2"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Addon made by Husham Memar</summary>
    <description>The Husham Memar Video Guides: Addon made by Husham Memar http://www.husham.com</description>
   
    <platform>all</platform>
  </extension>
</addon>

<addon
  id="plugin.video.haydargames"
  version="1.0.3"
  name="HaydarGames"
  provider-name="Husham.com">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  	<import addon="script.module.buggalo" version="1.1.2"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Addon WSoul fork from Husham Memar</summary>
    <description>Please enjoy and visit the Memar games channels to give Haydar a thank you for his selection of game channels to watch
	The Haydar Memar Games Youtube Channels: Addon made by Husham Memar http://www.husham.com:
	This Addon was a collection of all the Games chanels in youtube watched by kids these days:
	Haydar Memar wanted to share his gammes channels with everyone:
	Haydar Memar usually play games and post them in the memar games channels:
	P.S. This addon will only work with KODI Helix or above
		</description>
   
    <platform>all</platform>
  </extension>
</addon>


</addons>
